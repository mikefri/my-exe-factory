{
  "name": "usine-exe",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "build": "electron-builder --win"
  },
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "latest"
  },
  "build": {
    "appId": "com.monusine.app",
    "productName": "Mon Application Web",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable"
    },
    "files": [
      "main.js",
      "src/**/*"
    ]
  }
}
